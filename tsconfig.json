{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitOverride": true,
      "module": "es2020",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "Node",
      "downlevelIteration" : true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "lib": [
        "es2020",
        "dom"
      ]
    }
}
